function janrainSignOut() {
	janrain.capture.ui.endCaptureSession();
}
(function() {
	if ( typeof window.janrain !== 'object' ) {
		window.janrain = {};
	}
	window.janrain.settings = {};
	window.janrain.settings.capture = {};

	// capture settings
	janrain.settings.capture.redirectUri = "https://postmediawindsorstar2.wordpress.com/wp-admin/admin-ajax.php?action=janrain_capture_redirect_uri";
	janrain.settings.capture.appId = "ztcpr5854s8kyh93khrgw44sv9";
	janrain.settings.capture.clientId = "jwze6dse55fgqchrwptq7cq2a5paqxdg";
